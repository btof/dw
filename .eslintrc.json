{
	"env": {
			"browser": true,
			"es2021": true
	},
	"extends": [
			"standard"
	],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
			"ecmaVersion": 12,
			"sourceType": "module"
	},
	"plugins": [
			"@typescript-eslint"
	],
	"rules": {
		"semi": "off",
		"no-return-assign": "off",
		"no-unmodified-loop-condition": "off"
	}
}